import sys, random

var n = random.range(1...100)

while
{
    var guess = number(input("Pick a number from 1 to 100: "))
    if(guess == n)
    {
        print("You guessed it!")
        break;
    }
    else if(guess < n)
    {
        print("Too low")
    }
    else
    {
        print("Too high")
    }
}