class Outer 
{
    method() 
    {
        print(this) // expect: Outer instance

        function f() 
        {
            print(this) // expect: Outer instance

            class Inner 
            {
                method() 
                {
                    print(this) // expect: Inner instance
                }
            }

            Inner().method()
        }
        f()
    }
}

Outer().method()
