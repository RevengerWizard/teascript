class Counter 
{
    new(start, end) 
    {
        self.start = start
        self.end = end
        self.current = start
    }

    function iterate(iter) 
    {
        if self.current <= self.end
        {
            iter = self.current
            self.current += 1
            return iter
        } 
    }

    function iteratorvalue(iter) 
    {
        return iter
    }
}

var counter = Counter.new(1, 100)
for const i in counter
{
    print(i)
}