class Counter 
{
    new(start, end) 
    {
        self.start = start
        self.end = end
        self.current = start
    }

    function iter()
    {
        return function()
        {
            if self.current <= self.end
            {
                var i = self.current
                self.current += 1
                return i
            }
        }
    }
}

var counter = Counter.new(1, 100)
for const i in counter
{
    print(i)
}