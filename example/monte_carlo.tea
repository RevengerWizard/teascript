import random

function monte_carlo(nsamples)
{
    var acc = 0
    var i = nsamples
    while (i -= 1) > 0
    {
        var x = random.random()
        var y = random.random()
        if (x ** 2 + y ** 2) < 1.0
        {
            acc += 1
        }
    }
    return 4.0 * acc / nsamples
}

print(monte_carlo(10000))