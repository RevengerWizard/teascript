from io import stdout

for(var y_pixel in 0..24)
{
    var y = y_pixel / 12 - 1
    for (var x_pixel in 0..80)
    {
        var x = x_pixel / 30 - 2
        var x0 = x
        var y0 = y
        var iter = 0
        while(iter < 11 and x0 * x0 + y0 * y0 <= 4)
        {
            var x1 = (x0 * x0) - (y0 * y0) + x
            var y1 = 2 * x0 * y0 + y
            x0 = x1
            y0 = y1
            iter++
        }
        stdout.write(" .-:;+=xX$& "[iter])
    }
    print("")
}